{
  "remoteUser": "vscode",
  "workspaceFolder": "/workspaces/dam/${{ DEVCONTAINER_DIR_NAME }}",
  "mounts": [
    "source=${{ localWorkspaceFolder }},target=/workspaces/dam,type=bind,consistency=cached"
  ],
  "extensions": [
    "ms-vscode-remote.remote-containers",
    "ms-azuretools.vscode-docker",
    "dbaeumer.vscode-eslint",
    "redhat.vscode-yaml",
    "VisualStudioExptTeam.vscodeintellicode",
    "esbenp.prettier-vscode"
  ]
}
